<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<!-- 当前网市场的版本，不加前面的v，单纯只是版本号 -->
	<version>3.8</version>
	
	<!-- websocket im 的socket url访问地址 -->
	<!-- <websocketUrl>ws://wangzhanecs.wangshichang.com:81/websocket</websocketUrl> -->
	<websocketUrl>http://localhost:8081/GitHubWangMarket_im/websocket</websocketUrl>

	<!-- 阿里云短信发送 -->
	<AliyunSMSUtil>
		<!-- 是否使用此功能。若使用则此为true，若不使用，则下面参数不用管，use为false即可 -->
		<use>true</use>
		<regionId>cn-hongkong</regionId>
		<accessKeyId></accessKeyId>
		<accessKeySecret></accessKeySecret>
		
		<!-- 短信签名 -->
		<signName>网市场</signName>
		<!-- 登陆的短信模版 -->
		<login_templateCode>SMS_44315702</login_templateCode>
		<!-- 向代理商帐户转移站币，通知接收站币的乙方 -->
		<agency_siteSizeRecharge_templateCode>SMS_70095340</agency_siteSizeRecharge_templateCode>
		<!-- 网站使用期限延期(续费)后提醒对方 
			<siteYanQi_templateCode>SMS_71035143</siteYanQi_templateCode>
		-->
	</AliyunSMSUtil>
	
	<!-- 域名服务器列表 -->
	<domainServer>
		<!-- 香港 -->
		<domain>http://domain2.wang.market/</domain>
		<!-- 华北3 -->
		<domain>http://domainhuabei3.wang.market/</domain>
	</domainServer>
	
</configuration>