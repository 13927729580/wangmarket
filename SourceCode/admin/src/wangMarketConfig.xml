	<!-- 主站url，本站的URL -->
	<!-- <masterSiteUrl>http://wang.market/</masterSiteUrl> -->
	<masterSiteUrl>http://localhost:8082/GitHubWangMarket_admin/</masterSiteUrl>
	
	<!-- websocket im 的socket url访问地址 -->
