<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!--

    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/

-->
<urlrewrite>
    <rule>
        <note>
            dns域名重写
        </note>
        <from>^/(\w+).html$</from>
        <to type="forward">/dns.do?htmlFile=$1</to>
    </rule>
    <rule>
        <note>
            访问跟目录，也就是域名直接访问
        </note>
        <from>^/$</from>
        <to type="forward">/dns.do?htmlFile=index.html</to>
    </rule>
    <rule>
        <note>
            sitemap.xml
        </note>
        <from>sitemap.xml</from>
        <to type="forward">/sitemap.do</to>
    </rule>
    
    <rule>
        <from>^/sitePc/(\w+).html$</from>  
        <to>/site/html.do?htmlFile=$1</to> 
    </rule>
    
    <rule>
        <from>^/news/(\w+).html$</from>  
        <to>/site/html.do?htmlFile=$1</to> 
    </rule>
</urlrewrite>